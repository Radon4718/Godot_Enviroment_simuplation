; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="the nature of code"
run/main_scene="res://scenes/world.tscn"
config/features=PackedStringArray("4.3", "Mobile")
config/icon="res://icon.svg"

[display]

window/size/viewport_width=1200
window/size/viewport_height=720
window/size/resizable=false

[dotnet]

project/assembly_name="the nature of code"

[editor_plugins]

enabled=PackedStringArray("res://addons/ProceduralSceneGenerator2D/plugin.cfg", "res://addons/proton_control_animation/plugin.cfg")

[global_group]

Fish=""
Bug=""

[layer_names]

2d_render/layer_1="alive"
2d_render/layer_2="sight"
2d_render/layer_3="objects"
2d_physics/layer_1="alive"
2d_physics/layer_2="sight"
2d_physics/layer_3="inanimate"

[rendering]

renderer/rendering_method="mobile"
viewport/hdr_2d=true
